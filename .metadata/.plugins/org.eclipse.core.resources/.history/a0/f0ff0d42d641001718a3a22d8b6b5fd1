package test.mypac;

public class CountThread extends Thread{
	//멤버필드 정의하기
	private String name;
	private int count;
	//생성자
	public CountThread(String name,int count){
		this.name=name;
		this.count=count;
	}
	
	//스레드가 시작되었을 때 호출되는 메소드
	@Override
	public void run() {
		System.out.println(this.name+" 스레드 시작");
	}
}
